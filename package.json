		"@nestjs/platform-express": "^11.1.3",
		"@nestjs/platform-socket.io": "^11.1.3",
		"@nestjs/platform-ws": "^11.1.3",
		"@types/bun": "latest",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.2"
	},
	"files": [
		"src"
	],
	"typesVersions": {
		"*": {
