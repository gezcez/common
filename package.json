{
	"name": "@gezcez/common",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"version": "1.1.3",
	"repository": {
		"type": "git",
		"url": "https://github.com/gezcez/common-utils.git"
	},
	"type": "module",
	"devDependencies": {},
	"scripts": {
		"move:src": "bun run scripts/move-src.ts",
